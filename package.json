{
  "name": "cap_northwind_mochatest",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^3.2.0",
    "@sap/cds": "^6",
    "@sap/xsenv": "^3.4.0",
    "@sap/xssec": "^3",
    "express": "^4",
    "passport": "^0"
  },
  "devDependencies": {
    "@sap/cds-dk": "^7",
    "chai": "^4.3.7",
    "chai-http": "^4.4.0",
    "mocha": "^10.2.0",
    "sqlite3": "^5"
  },
  "scripts": {
    "test": "mocha tests/test.js --timeout 15000 --exit",
		"start": "npx cds run"
  },
  "cds": {
    "requires": {
      "NorthWind": {
        "kind": "odata",
        "model": "srv/external/NorthWind"
      },
      "so_capsrv": {
        "kind": "odata",
        "model": "srv/external/so_capsrv"
      }
    },
    "[local]": {
      "requires": {
        "NorthWind": {
          "kind": "odata",
          "model": "srv/external/NorthWind",
          "credentials": {
            "destination": "NorthWind"
          }
        },
        "so_capsrv": {
          "kind": "odata",
          "model": "srv/external/so_capsrv",
          "credentials": {
            "destination": "SO_CAP_SRV",
            "path": "/catalog/"
          }
        }
      }
    },
    "[production]": {
      "requires": {
        "NorthWind": {
          "kind": "odata",
          "model": "srv/external/NorthWind",
          "credentials": {
            "destination": "NorthWind"
          }
        },
        "so_capsrv": {
          "kind": "odata",
          "model": "srv/external/so_capsrv",
          "credentials": {
            "destination": "SO_CAP_SRV",
            "path": "/catalog/"
          }
        }
      }
    }
  }
}
